{
  "name": "server",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "packageManager": "yarn@3.4.1",
  "scripts": {
    "build": "cd ../client && yarn build",
    "client:dev": "cd ../client && yarn start:dev",
    "server:dev": "nodemon -r dotenv/config server.js",
    "start:dev": "NODE_ENV=development concurrently --kill-others-on-fail \"yarn server:dev\" \"yarn client:dev\"",
    "start:staging": "NODE_ENV=staging node -r dotenv/config server.js",
    "start": "NODE_ENV=production node -r dotenv/config server.js"
  },
  "dependencies": {
    "axios": "^1.3.2",
    "body-parser": "^1.20.1",
    "compression": "^1.7.4",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-favicon": "^2.0.1",
    "fs": "^0.0.1-security",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.20"
  }
}
